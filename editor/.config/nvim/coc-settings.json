{
	"languageserver": {
		"golang": {
			"command": "gopls",
			"rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
			"filetypes": ["go"],
			"initializationOptions": {
				"usePlaceholders": true
			}
		},
		"vala": {
			"command": "vala-language-server",
			"filetypes": ["vala", "genie"]
		}
	}
}
